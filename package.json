{
	"name": "@orc-hfg/uploader",
	"type": "module",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/orc-hfg/uploader.git"
	},
	"engines": {
		"node": "22.16.0"
	},
	"scripts": {
		"build": "nuxt build",
		"build:ci": "npm run build",
		"dev": "nuxt dev",
		"staging": "nuxt dev --envName staging --dotenv .env.staging",
		"generate": "nuxt generate",
		"deploy:development": "sh ./deployment/uploader-deploy.sh development",
		"deploy:staging": "sh ./deployment/uploader-deploy.sh staging",
		"start:sentry": "node --import ./.output/server/sentry.server.config.mjs .output/server/index.mjs",
		"preview": "NUXT_PREVIEW=true nuxt preview",
		"preview:ci": "npm run preview",
		"preview:build": "NUXT_PREVIEW=true npm run build && npm run preview",
		"preview:production": "NUXT_PREVIEW=true npm run build && node .output/server/index.mjs",
		"preview:production:sentry": "NUXT_PREVIEW=true NUXT_PUBLIC_SERVER_URL=http://localhost:3000/ NUXT_PUBLIC_SENTRY_ALLOW_HOSTNAME=localhost NUXT_PUBLIC_SENTRY_ENABLED=true npm run build && npm run start:sentry",
		"postinstall": "nuxt prepare",
		"setup-npmrc": "node ./scripts/setup-npmrc.mjs",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:e2e": "playwright test",
		"test:e2e:codegen": "playwright codegen localhost:3000",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:headed:slowmo": "SLOWMO=2000 playwright test --headed",
		"check:eslint": "npx @eslint/config-inspector@latest",
		"check:types": "nuxt typecheck",
		"check:unused": "NODE_NO_WARNINGS=1 knip",
		"check:issues": "concurrently \"npm run lint\" \"npm run check:types\" \"npm run check:unused\" \"npm run test\"",
		"check:node": "node ./scripts/check-node-version.mjs",
		"check:updates": "npm-check-updates --interactive --format group",
		"upgrade": "nuxt upgrade",
		"madek-api-nuxt-layer:link": "npm link @orc-hfg/madek-api-nuxt-layer",
		"madek-api-nuxt-layer:unlink": "npm unlink @orc-hfg/madek-api-nuxt-layer --no-save && npm install",
		"prepare": "husky"
	},
	"dependencies": {
		"@nuxtjs/google-fonts": "3.2.0",
		"@nuxtjs/i18n": "9.5.5",
		"@orc-hfg/madek-api-nuxt-layer": "1.3.9",
		"@primeuix/themes": "1.1.1",
		"@primevue/forms": "4.3.5",
		"@sentry/nuxt": "9.34.0",
		"@vueuse/nuxt": "13.3.0",
		"http-status-codes": "2.3.0",
		"nuxt": "3.17.5",
		"primeicons": "7.0.0",
		"primevue": "4.3.5",
		"tailwindcss-primeui": "0.6.1",
		"vue": "3.5.16",
		"vue-router": "4.5.1"
	},
	"devDependencies": {
		"@antfu/eslint-config": "4.14.1",
		"@nuxt/devtools": "npm:@nuxt/devtools-nightly@latest",
		"@nuxt/eslint": "1.4.1",
		"@nuxt/test-utils": "3.19.1",
		"@playwright/test": "1.54.1",
		"@primevue/nuxt-module": "4.3.5",
		"@tailwindcss/vite": "4.1.10",
		"@types/node": "24.0.1",
		"@vue/test-utils": "2.4.6",
		"concurrently": "9.1.2",
		"eslint": "9.28.0",
		"eslint-plugin-better-tailwindcss": "3.1.0",
		"eslint-plugin-playwright": "2.2.0",
		"eslint-plugin-sonarjs": "3.0.2",
		"eslint-plugin-vuejs-accessibility": "2.4.1",
		"happy-dom": "17.4.6",
		"husky": "9.1.7",
		"knip": "5.57.2",
		"lint-staged": "16.1.0",
		"npm-check-updates": "18.0.1",
		"playwright-core": "1.53.0",
		"tailwindcss": "4.1.10",
		"type-fest": "4.41.0",
		"typescript": "5.8.3",
		"vite-plugin-devtools-json": "0.2.0",
		"vitest": "3.2.3",
		"vue-tsc": "2.2.10"
	},
	"overrides": {
		"@vercel/nft": "^0.27.4"
	}
}
