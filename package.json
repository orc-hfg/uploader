{
	"name": "@orc-hfg/uploader",
	"type": "module",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/orc-hfg/uploader.git"
	},
	"engines": {
		"node": "22.15.0"
	},
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"staging": "nuxt dev --envName staging --dotenv .env.staging",
		"generate": "nuxt generate",
		"deploy:dev": "sh ./deployment/uploader-do-dist-dev.sh",
		"deploy:staging": "sh ./deployment/uploader-do-dist-staging.sh",
		"preview": "NUXT_APP_BASE_URL=/uploader/ NUXT_ENABLE_DEBUG_LOGGING=true nuxt preview",
		"preview:build": "npm ci && npm run build && NUXT_APP_BASE_URL=/uploader/ NUXT_ENABLE_DEBUG_LOGGING=true nuxt preview",
		"preview:production": "npm ci && npm run build && NUXT_APP_BASE_URL=/uploader/ NUXT_ENABLE_DEBUG_LOGGING=true node .output/server/index.mjs",
		"postinstall": "nuxt prepare",
		"setup-npmrc": "node ./scripts/setup-npmrc.mjs",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"check:eslint": "npx @eslint/config-inspector@latest",
		"check:types": "nuxt typecheck",
		"check:unused": "knip",
		"check:issues": "concurrently \"npm run lint\" \"npm run check:types\" \"npm run check:unused\" \"npm run test\"",
		"check:node": "node ./scripts/check-node-version.mjs",
		"check:updates": "npm-check-updates --interactive --format group",
		"upgrade": "nuxt upgrade",
		"madek-api-nuxt-layer:link": "npm link @orc-hfg/madek-api-nuxt-layer",
		"madek-api-nuxt-layer:unlink": "npm unlink @orc-hfg/madek-api-nuxt-layer --no-save && npm install",
		"prepare": "husky"
	},
	"dependencies": {
		"@nuxtjs/google-fonts": "3.2.0",
		"@orc-hfg/madek-api-nuxt-layer": "1.1.16",
		"@primeuix/themes": "1.1.1",
		"@primevue/forms": "4.3.4",
		"nuxt": "3.17.2",
		"primeicons": "7.0.0",
		"primevue": "4.3.4",
		"tailwindcss-primeui": "0.6.1",
		"vue": "3.5.13",
		"vue-router": "4.5.1"
	},
	"devDependencies": {
		"@antfu/eslint-config": "4.13.0",
		"@nuxt/devtools": "npm:@nuxt/devtools-nightly@latest",
		"@nuxt/eslint": "1.3.0",
		"@nuxt/test-utils": "3.18.0",
		"@primevue/nuxt-module": "4.3.4",
		"@tailwindcss/vite": "4.1.5",
		"@types/node": "22.15.17",
		"@vue/test-utils": "2.4.6",
		"concurrently": "9.1.2",
		"dotenv": "16.5.0",
		"eslint": "9.26.0",
		"eslint-plugin-sonarjs": "3.0.2",
		"eslint-plugin-vuejs-accessibility": "2.4.1",
		"happy-dom": "17.4.6",
		"husky": "9.1.7",
		"knip": "5.55.1",
		"lint-staged": "15.5.2",
		"npm-check-updates": "18.0.1",
		"playwright-core": "1.52.0",
		"tailwindcss": "4.1.5",
		"typescript": "5.8.3",
		"vitest": "3.1.3",
		"vue-tsc": "2.2.10"
	}
}
