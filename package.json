{
	"name": "@orc-hfg/uploader",
	"type": "module",
	"version": "0.1.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/orc-hfg/uploader.git"
	},
	"engines": {
		"node": "22.18.0"
	},
	"scripts": {
		"build": "nuxt build",
		"build:preview": "npm install && nuxt build && npm run preview",
		"build:preview:sentry": "npm install && nuxt build && npm run preview:sentry",
		"dev": "nuxt dev",
		"link:dev": "npm run madek-api-nuxt-layer:link && echo '\nüîó Main application successfully linked to development layer!' && echo 'üìã Make sure the layer development server is running in parallel (run \"npm run link:dev\" in your madek-api-nuxt-layer repository folder).' && echo '‚ö†Ô∏è  Both development servers need to run for live updates.\n' && npm run dev",
		"staging": "nuxt dev --envName staging --dotenv .env.staging",
		"generate": "nuxt generate",
		"deploy:development": "sh ./deployment/uploader-deploy.sh development",
		"deploy:staging": "sh ./deployment/uploader-deploy.sh staging",
		"preview": "nuxt preview --dotenv .env.preview",
		"preview:sentry": "dotenv -e .env.preview.sentry -- node --import ./.output/server/sentry.server.config.mjs .output/server/index.mjs",
		"postinstall": "nuxt prepare",
		"setup-npmrc": "node ./scripts/setup-npmrc.mjs",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:e2e": "playwright test",
		"test:e2e:changed": "playwright test --only-changed=main",
		"test:e2e:failed": "playwright test --last-failed",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:ui": "playwright test --ui --headed --workers=1",
		"test:e2e:headed": "playwright test --headed --workers=1",
		"test:e2e:headed:slowmo": "SLOWMO=2000 playwright test --headed --workers=1",
		"test:e2e:codegen": "playwright codegen localhost:3000",
		"check:eslint": "npx @eslint/config-inspector@latest",
		"check:types": "nuxt prepare && vue-tsc -b --noEmit",
		"check:unused": "NODE_NO_WARNINGS=1 knip",
		"check:issues": "npm run lint && npm run check:types && npm run check:unused && npm run test && npm run test:e2e",
		"check:node": "node ./scripts/check-node-version.mjs",
		"check:updates": "npm-check-updates --interactive --format group",
		"upgrade": "nuxt upgrade",
		"madek-api-nuxt-layer:link": "npm link @orc-hfg/madek-api-nuxt-layer",
		"release:patch": "node ./scripts/safe-release.mjs patch",
		"release:minor": "node ./scripts/safe-release.mjs minor",
		"release:major": "node ./scripts/safe-release.mjs major",
		"prepare": "husky",
		"reinstall": "rm -rf node_modules package-lock.json && npm install"
	},
	"dependencies": {
		"@nuxtjs/google-fonts": "3.2.0",
		"@nuxtjs/i18n": "10.0.3",
		"@orc-hfg/madek-api-nuxt-layer": "1.6.11-dev.7",
		"@primeuix/themes": "1.2.3",
		"@primevue/forms": "4.3.7",
		"@sentry/nuxt": "10.1.0",
		"@vueuse/nuxt": "13.6.0",
		"consola": "3.4.2",
		"http-status-codes": "2.3.0",
		"nuxt": "4.0.3",
		"primeicons": "7.0.0",
		"primevue": "4.3.7",
		"tailwindcss-primeui": "0.6.1",
		"vue": "3.5.18",
		"vue-router": "4.5.1"
	},
	"devDependencies": {
		"@antfu/eslint-config": "5.2.0",
		"@axe-core/playwright": "4.10.2",
		"@nuxt/devtools": "npm:@nuxt/devtools-nightly@latest",
		"@nuxt/eslint": "1.8.0",
		"@nuxt/test-utils": "3.19.2",
		"@playwright/test": "1.54.2",
		"@primevue/nuxt-module": "4.3.7",
		"@tailwindcss/vite": "4.1.11",
		"@types/node": "24.2.0",
		"@vue/test-utils": "2.4.6",
		"dotenv-cli": "10.0.0",
		"eslint": "9.32.0",
		"eslint-plugin-better-tailwindcss": "3.7.4",
		"eslint-plugin-playwright": "2.2.2",
		"eslint-plugin-sonarjs": "3.0.4",
		"eslint-plugin-vuejs-accessibility": "2.4.1",
		"happy-dom": "18.0.1",
		"husky": "9.1.7",
		"knip": "5.62.0",
		"lint-staged": "16.1.4",
		"npm-check-updates": "18.0.2",
		"playwright-core": "1.54.2",
		"tailwindcss": "4.1.11",
		"type-fest": "4.41.0",
		"typescript": "5.8.3",
		"vite-plugin-devtools-json": "0.4.1",
		"vitest": "3.2.4",
		"vue-tsc": "3.0.5"
	},
	"overrides": {
		"@vercel/nft": "^0.30.0"
	}
}
