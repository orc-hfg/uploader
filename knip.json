{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"app/app.vue",
		"app/middleware/*.ts",
		"app/pages/**/*.vue",
		"app/stores/*.ts",
		"app/theme/*.ts",
		"app/layouts/*.vue",
		"app/types/*.ts",
		"tests/**/*.ts"
	],
	"project": [
		"app/**",
		"server/**",
		"shared/**",
		"tests/**"
	],
	"ignoreDependencies": [
		"@nuxt/devtools",
		"@nuxt/eslint",
		"@nuxtjs/google-fonts",
		"@nuxtjs/i18n",
		"@primevue/nuxt-module",
		"@vueuse/core",
		"@vueuse/nuxt",
		"ofetch",
		"primeicons",
		"tailwindcss-primeui",
		"vitest-environment-nuxt",
		"vue-tsc",
		"vue-router",
		"zod"
	]
}
