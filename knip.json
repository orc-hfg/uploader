{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"app/app.vue",
		"app/pages/**/*.vue",
		"app/theme/*.ts",
		"app/layouts/*.vue",
		"types/*.ts"
	],
	"project": [
		"app/**"
	],
	"ignore": [
		"app/composables/**",
		"app/middleware/**",
		"app/utils/**",
		"app/stores/**",
		"app/app.config.ts"

	],
	"ignoreDependencies": [
		"@nuxt/devtools",
		"@nuxt/eslint",
		"@nuxtjs/google-fonts",
		"@nuxtjs/i18n",
		"@orc-hfg/madek-api-nuxt-layer",
		"@primevue/nuxt-module",
		"@vueuse/core",
		"@vueuse/nuxt",
		"primeicons",
		"tailwindcss-primeui",
		"vitest-environment-nuxt",
		"vue",
		"vue-i18n",
		"vue-tsc",
		"vue-router",
		"zod"
	]
}
