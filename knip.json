{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"app/app.vue",
		"app/error.vue",
		"app/composables/*.ts",
		"app/layouts/*.vue",
		"app/middleware/*.ts",
		"app/pages/**/*.vue",
		"app/plugins/*.ts",
		"app/stores/*.ts",
		"app/theme/*.ts",
		"app/types/*.ts",
		"tests/**/*.ts"
	],
	"project": [
		"app/**",
		"server/**",
		"shared/**",
		"tests/**"
	],
	"ignoreDependencies": [
		"@nuxt/devtools",
		"@nuxtjs/html-validator",
		"@orc-hfg/madek-api-nuxt-layer",
		"@primevue/forms",
		"@vueuse/core",
		"ofetch",
		"primeicons",
		"tailwindcss-primeui",
		"vue-router",
		"zod"
	],
	"ignoreUnresolved": [
		"#app",
		"#components"
	],
	"paths": {
		"@/*": [
			"app/*"
		],
		"@@/*": [
			"./*"
		]
	},
	"vitest": false
}
