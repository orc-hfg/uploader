{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"app/app.vue",
		"app/composables/*.ts",
		"app/middleware/*.ts",
		"app/pages/**/*.vue",
		"app/stores/*.ts",
		"app/theme/*.ts",
		"app/layouts/*.vue",
		"app/types/*.ts",
		"tests/**/*.ts"
	],
	"project": [
		"app/**",
		"server/**",
		"shared/**",
		"tests/**"
	],
	"ignoreDependencies": [
		"@nuxt/devtools",
		"@nuxt/eslint",
		"@nuxtjs/google-fonts",
		"@nuxtjs/i18n",
		"@primevue/nuxt-module",
		"@vueuse/core",
		"@vueuse/nuxt",
		"dotenv",
		"ofetch",
		"primeicons",
		"tailwindcss-primeui",
		"vue-router",
		"zod"
	],
	"ignoreBinaries": [
		"dotenv"
	],
	"ignoreUnresolved": [
		"#app"
	],
	"paths": {
		"@/*": [
			"app/*"
		],
		"@@/*": [
			"./*"
		]
	},
	"vitest": false
}
