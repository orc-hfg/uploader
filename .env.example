# GitHub Personal Access Token for accessing GitHub packages
# Required for @orc-hfg packages
# Required permissions: read:packages
# After changing this token, run: npm run setup-npmrc
GITHUB_PAT=

# Token for accessing the Madek API in development mode
NUXT_MADEK_API_TOKEN=

# SSH User for deployment
MADEK_SSH_USER=

# The SENTRY_AUTH_TOKEN variable is picked up by the Sentry Build Plugin.
# It's used for authentication when uploading source maps.
SENTRY_AUTH_TOKEN=

# NOTE: For staging environment, create a separate .env.staging file with:
# - NUXT_MADEK_API_TOKEN (staging-specific token)
# - NUXT_PUBLIC_SERVER_URL=https://staging.madek.hfg-karlsruhe.de/
